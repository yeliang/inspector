<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >

    <EditText
        android:id="@+id/editMeid"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content" 
        
    />
    
    <TextView  
    	android:id="@+id/txtFullLicense" 
    	android:layout_width="fill_parent"
    	android:layout_height="wrap_content"
    	android:textColor="#FF0000"
    	android:textSize="24dp"
    />
        
    <Button android:id="@+id/btn_generate" android:text="@string/generate" android:layout_height="wrap_content" android:layout_width="140dp" />
    <Button android:id="@+id/btn_sendfull" android:text="@string/sendsms" android:layout_height="wrap_content" android:layout_width="140dp" />
    
</LinearLayout>
